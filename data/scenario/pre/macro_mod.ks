[endif]
[if exp="f.ngay>=30 || f.money_day==1"]
[eval exp="f.money_day=0"]
[call storage="mod/money_day.ks" target="*return_money"][endif]
[call storage="mod/money_day.ks" target="*chi_phi"]
[if exp="f.lunch_check==1"]
[eval exp="f.lunch_check=0"][else]
[call storage="mod/dinner.ks" target="*check_lunch"][endif]
[if exp="f.money_owe>=1 && f.ngay>=10"]
[call storage="mod/money_day.ks" target="*money_owe"][endif]
